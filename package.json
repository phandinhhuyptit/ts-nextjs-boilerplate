{
  "name": "int-nextjs-ts",
  "version": "1.0.0",
  "engines": {
    "node": "12.16.1",
    "npm": "6.13.4"
  },
  "scripts": {
    "dev": "nodemon --exec ts-node --project tsconfig-server.json server.ts",
    "lint": "npm run prettier:check && gts check",
    "lint:fix": "gts fix && npm run prettier:fix",
    "test": "echo \"Need to write test\"",
    "prebuild": "npm run lint:fix && npm run test && npm run clean",
    "build": "npm run build:server && npm run build:client && npm run build:bundle",
    "build:server": "tsc -p tsconfig-server.json && cp {package,package-lock}.json build",
    "build:client": "next build && cp -R .next build && cp -R static build/static",
    "build:bundle": "mkdir -p build-bundle && tar -cvzf build-bundle/bundle.tar.gz build",
    "clean": "gts clean && rm -rf .next build",
    "check": "gts check",
    "compile": "tsc -p .",
    "fix": "gts fix",
    "prepare": "npm run compile",
    "pretest": "npm run compile",
    "posttest": "npm run check",
    "prettier:cli": "pretty-quick --staged",
    "prettier:check": "npm run prettier:cli",
    "prettier:fix": "npm run prettier:cli"
  },
  "dependencies": {
    "@coreui/coreui": "^3.0.0-beta.4",
    "@coreui/react": "^2.5.4",
    "@zeit/next-sass": "^0.2.1-canary.4",
    "axios": "^0.19.0",
    "bootstrap": "^4.3.1",
    "classnames": "^2.2.6",
    "cookie": "^0.4.0",
    "cookie-parser": "^1.4.4",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "formik": "^1.5.8",
    "http-proxy-middleware": "^0.19.1",
    "js-cookie": "^2.2.1",
    "lodash": "^4.17.15",
    "next": "^9.1.6",
    "next-redux-wrapper": "^3.0.0-alpha.3",
    "node-sass": "^4.12.0",
    "nprogress": "^0.2.0",
    "qs": "^6.9.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.1.0",
    "react-table": "^7.0.0-rc.16",
    "reactstrap": "^8.0.0",
    "recompose": "^0.30.0",
    "redux": "^4.0.1",
    "redux-crud": "^3.3.0",
    "redux-thunk": "^2.3.0",
    "toastr": "^2.1.4",
    "validate.js": "^0.13.1",
    "validator": "^12.0.0",
    "yup": "^0.27.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.8.3",
    "@babel/plugin-proposal-optional-chaining": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "@commitlint/cli": "^8.3.3",
    "@commitlint/config-conventional": "^8.0.0",
    "@types/classnames": "^2.2.9",
    "@types/cookie-parser": "^1.4.1",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.0",
    "@types/http-proxy-middleware": "^0.19.2",
    "@types/js-cookie": "^2.2.4",
    "@types/lodash": "^4.14.135",
    "@types/next": "^9.0.0",
    "@types/node": "^10.0.3",
    "@types/nprogress": "^0.2.0",
    "@types/react": "^16.8.22",
    "@types/react-dom": "^16.8.4",
    "@types/react-redux": "^7.1.0",
    "@types/react-table": "^7.0.6",
    "@types/reactstrap": "^8.0.1",
    "@types/recompose": "^0.30.7",
    "@types/redux-thunk": "^2.1.0",
    "@types/toastr": "^2.1.37",
    "@types/validator": "^10.11.3",
    "@types/yup": "^0.26.22",
    "eslint": "^6.8.0",
    "eslint-plugin-prettier": "^3.1.1",
    "gts": "^1.1.2",
    "husky": "^2.3.0",
    "nodemon": "^1.19.1",
    "prettier": "1.19.1",
    "pretty-quick": "^2.0.1",
    "stylelint": "^10.1.0",
    "stylelint-checkstyle-formatter": "^0.1.2",
    "stylelint-config-standard": "^18.3.0",
    "stylelint-scss": "^3.8.0",
    "ts-node": "^8.6.2",
    "typescript": "^3.7.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
